{
  "version": 2,
  "name": "genifydesigns",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "app/api/generate/route.ts": {
      "maxDuration": 30
    },
    "app/api/checkout/route.ts": {
      "maxDuration": 10
    },
    "app/api/webhooks/stripe/route.ts": {
      "maxDuration": 10
    }
  }
} 